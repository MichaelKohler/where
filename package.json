{
  "name": "where",
  "version": "1.0.0",
  "description": "Where is Michael?",
  "author": "Michael Kohler",
  "license": "MPL-2.0",
  "main": "main.js",
  "dependencies": {
    "@babel/preset-env": "^7.13.9",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "webpack-cli": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.9",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.13.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.24.4",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "build": "./node_modules/webpack/bin/webpack.js --mode production",
    "start": "webpack serve --mode development --env development",
    "eslint": "eslint src tests",
    "test": "npm run eslint && jest"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/main.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 81,
        "functions": 77,
        "lines": 83,
        "statements": 84
      }
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.scss$": "<rootDir>/tests/file-transformer.js"
    },
    "moduleNameMapper": {
      "trips.json": "<rootDir>/tests/__mocks__/trips.json"
    },
    "setupFiles": [
      "<rootDir>/tests/test-setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
